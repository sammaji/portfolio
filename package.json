{
  "name": "sammaji.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cl": "contentlayer build",
    "format": "prettier --write \"{app,components,lib}/**/*.{tsx,ts}\" \"posts/*.mdx\"",
    "dev:tr": "next dev --turbo",
    "dev:cl": "concurrently \"contentlayer dev --clearCache --verbose\" \"next dev -p 3000\"",
    "build": "contentlayer build --clearCache && next build",
    "start": "next start",
    "deploy": "vercel .",
    "deploy:gh": "tsc && vite build && gh-pages -d build"
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1",
      "@opentelemetry/core": "1.13.0",
      "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
      "@opentelemetry/resources": "1.13.0",
      "@opentelemetry/sdk-trace-base": "1.13.0",
      "@opentelemetry/sdk-trace-node": "1.13.0",
      "@opentelemetry/semantic-conventions": "1.13.0"
    }
  },
  "dependencies": {
    "@emailjs/browser": "^3.10.0",
    "@emotion/react": "^11.10.6",
    "@gitgraph/react": "^1.6.0",
    "@icons-pack/react-simple-icons": "^9.5.0",
    "@mantine/core": "^6.0.0",
    "@mantine/hooks": "^6.0.0",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/node": "20.4.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cmdk": "^1.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "framer-motion": "^11.2.4",
    "lucide-react": "^0.260.0",
    "next": "13.4.9",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.3.0",
    "open-graph-scraper": "^6.5.1",
    "parse-open-graph": "^0.4.0",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-spring": "^9.7.3",
    "react-wrap-balancer": "^1.1.0",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-pretty-code": "^0.10.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^0.14.5",
    "sonner": "^1.4.41",
    "tailwind-merge": "^1.13.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.6",
    "vaul": "^0.9.1",
